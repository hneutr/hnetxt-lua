<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>golf.hne.dailycommit.plist</string>
    <key>Program</key>
    <string>/Users/hne/lib/jobs/daily-commit.sh</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>SHELL</key>
        <string>/bin/zsh</string>
        <key>HOME</key>
        <string>/Users/hne</string>
        <key>DOTFILES</key>
        <string>/Users/hne/dotfiles</string>
    </dict>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key><integer>6</integer>
        <key>Minute</key><integer>0</integer>
    </dict>

    <key>StandardErrorPath</key>
    <string>/Users/hne/.data/.jobs/err/daily-commit.txt</string>
    <key>StandardOutPath</key>
    <string>/Users/hne/.data/.jobs/out/daily-commit.txt</string>
</dict>
</plist>
